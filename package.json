{
  "name": "ui-Libary",
  "version": "0.2.0",
  "description": " libraries",
  "main": "./dist/index.js",
  "repository": "https://gitlab.xdealflow.com/tranzact/xdealflow-ui-libary",
  "author": "Shawn Brinkman",
  "scripts": {
    "build:dev": "rollup -c rollup.config.dev.js ",
    "build:prod": "rollup -c rollup.config.prod.js ",
    "clean": "rimraf dist/index.*.* dist/chunk-*.js dist/*.css packages/*/dist/index.*.* packages/*/dist/chunk-*.js packages/*/dist/*.css",
    "build": "yarn run clean && yarn run build:prod --environment PACKAGES:*",
    "watch": "yarn run build:dev --w",
    "start:local": "concurrently \"npm:watch -- --environment PACKAGES:*\" \"npm:storybook\"",
    "start": "npm run storybook",
    "test": "jest --passWithNoTests",
    "lint": "yarn run lint:css && yarn run lint:js",
    "lint:js": "eslint \"**/*.js\"",
    "storybook": "start-storybook -p 8484 -c .storybook",
    "build-storybook": "build-storybook -c .storybook -o storybook-static",
    "bootstrap": "lerna bootstrap",
    "dist": "npm run build && lerna publish"
  },
  "resolutions": {
    "@babel/preset-env": "7.5.5"
  },
  "jest": {
    "setupFiles": [
      "./test-config.js"
    ],
    "collectCoverageFrom": [
      "(src|packages)/**/*.js",
      "!(src|packages)/**/(index|*.stories|stories).js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file.mock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/style.mock.js"
    }
  },
